#include <iostream>
using namespace std;

int main()
{
    int n,f,f1=-1,f2=1;
    // clrscr();
     
    cout<<"  Enter The Number Of Terms:";
    cin>>n;
     
    cout<<"  The Fibonacci Series is:";
     
    do
    {
        f=f1+f2;
        f1=f2;
        f2=f;
        cout<<"  \n"<<f;
        n--;
    }while(n>0);
    // getch();
}
